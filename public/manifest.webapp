{
  "name": "KaiFace",
  "description": "Biometric Face identification and verification for KaiOS",
  "version": "1.0",
  "developer": {
    "name": "Elliot Laidlaw, Fraser Barton",
    "url": "https://github.com/elliotlaidlaw/face-recognition-kaios/"
  },
  "launch_path": "/index.html",
    "icons": {
    "56": "/icons/app_56.png",
    "112": "/icons/app_112.png",
    "128": "/icons/app_128.png"
  },
  "categories": ["utilities"],
  "theme_color": "#a065e6",
  "type": "privileged",
  "permissions": {
    "systemXHR": {},
    "camera": {
      "description": "Required for capturing user's face for recognition"
    },
    "spatialnavgigation-app-manage": {}
  },
  "fullscreen": "true",
  "chrome": {
    "statusbar": "overlap"
  },
  "orientation": [ "portrait-primary" ],
  "default_locale": "en-GB"
}
